  "dependencies": {
    "vue": "^3.0.5",
    "vue-router": "^4.0.3"
  },
  "devDependencies": {
    "@rollup/plugin-yaml": "^3.0.0",
    "@types/fs-extra": "^9.0.6",
    "@types/js-yaml": "^4.0.0",
    "@types/node": "^14.14.22",
    "@vitejs/plugin-vue": "^1.1.2",
    "@vue/compiler-sfc": "^3.0.5",
    "@vuedx/typecheck": "^0.6.0",
    "@vuedx/typescript-plugin-vue": "^0.6.0",
    "babel-eslint": "^10.1.0",
    "chokidar": "^3.5.1",
    "eslint": "^7.18.0",
    "eslint-plugin-vue": "^7.5.0",
    "fs-extra": "^9.1.0",
    "js-yaml": "^4.0.0",
    "typescript": "^4.1.3",
    "vite": "^2.0.0-beta.50",
    "vite-plugin-components": "^0.6.6"
